<h1 class="center"><%= @billboard.name %></h1>
<hr />
<h4 class="center">Top Songs</h4>
<%= link_to 'Add Song', new_billboard_song_path(@billboard), class: "btn black white-text" %>
</div>
<div class="container">
  <div class="row">
    <% @billboard.songs.each do |song| %>
      <div>
        <%= link_to song.title, billboard_song_path(@billboard, song)%>
      </div>
    <% end %>
  </div>
</div>
<%= link_to "Billboards", billboards_path, class: "btn orange black-text" %>
