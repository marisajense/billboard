<h1 class="center">Add a Billboard</h1>
<div class="container">
  <div class="row">
    <%= form_for @billboard do |f| %>
      <div class="col m6">
        <%= f.label :name %>
        <%= f.text_field :name, required: true %>
      </div>
      <%= f.submit class: "btn black white-text" %>
    <% end %>
  </div>
</div>
